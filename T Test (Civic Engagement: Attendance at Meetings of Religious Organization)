# Calculate simple averages for Bolivia and Paraguay
average_score_bolivia <- mean(c(48.17, 51.23, 6.99))
average_score_paraguay <- mean(c(57.00, 52.55, 11.36))
# Assign weights (for example: 0.5 for Neighborhood Insecurity, 0.3 for Trust, 0.2 for Attendance)
weights <- c(0.5, 0.3, 0.2)
# Calculate weighted averages for Bolivia and Paraguay
weighted_score_bolivia <- sum(weights * c(48.17, 51.23, 6.99))
weighted_score_paraguay <- sum(weights * c(57.00, 52.55, 11.36))
# Replace the placeholder with actual aggregate scores
aggregate_data <- data.frame(
Country = c("Bolivia", "Paraguay"),
Aggregate_Score = c(average_score_bolivia, average_score_paraguay) # Insert your calculated scores here
)
# Print to check
print(aggregate_data)
# Visualize the aggregate differences
ggplot(aggregate_data, aes(x = Country, y = Aggregate_Score, fill = Country)) +
geom_bar(stat = "identity") +
labs(title = "Aggregate Differences Across Democratic Support Factors",
x = "Country",
y = "Aggregate Score") +
theme_minimal() +
theme(plot.title = element_text(hjust = 0.5)) # Center the plot title
# Data setup
bolivia_data <- c(40.16, 39.61, 40.61, 43.00, 46.79, 49.58, 47.06, 55.76)
paraguay_data <- c(47.29, 47.74, 50.78, 56.15, 55.47, 52.99, 43.38, 51.88)
# Perform t-test
t_test_result <- t.test(bolivia_data, paraguay_data, paired = TRUE)
# Print t-test result
print(t_test_result)
# Combine the data into a data frame for visualization
attendance_data <- data.frame(
Country = rep(c("Bolivia", "Paraguay"), each = 8),
Year = rep(c(2023, 2018, 2016, 2014, 2012, 2010, 2008, 2006), 2),
Attendance = c(bolivia_data, paraguay_data)
)
# Create the boxplot with data points
ggplot(attendance_data, aes(x = Country, y = Attendance, fill = Country)) +
geom_boxplot() +
geom_jitter(color = "black", size = 1, width = 0.2) +
labs(title = "Civic Engagement: Attendance at Meetings of Religious Organization",
x = "Country",
y = "Attendance Score") +
theme_minimal() +
theme(plot.title = element_text(hjust = 0.5)) # Center the plot title



