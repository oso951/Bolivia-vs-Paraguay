Executive_Legislature = runif(40, min = 20, max = 80),
Dissolve_Supreme = runif(40, min = 20, max = 80),
Neighborhood_Insecurity = runif(40, min = 20, max = 80),
Satisfaction_Democracy = runif(40, min = 20, max = 80),
Victim_Crime = runif(40, min = 20, max = 80),
Victim_Crime_Last_Year = runif(40, min = 20, max = 80)
)
# Reshape the data into a long format
long_data <- data %>%
pivot_longer(
cols = c(Confidence_Judiciary, Coup_Corruption, Coup_Crime, Democracy_Support,
Executive_Legislature, Dissolve_Supreme, Neighborhood_Insecurity,
Satisfaction_Democracy, Victim_Crime, Victim_Crime_Last_Year),
names_to = "Category",
values_to = "Score"
)
# Function to perform ANOVA and print the results
perform_anova <- function(data, category) {
cat("ANOVA for", category, ":\n")
anova_result <- aov(Score ~ Country + Year, data = filter(data, Category == category))
print(summary(anova_result))
cat("\n")
}
# Perform ANOVA for each category
unique_categories <- unique(long_data$Category)
map(unique_categories, ~perform_anova(long_data, .))
