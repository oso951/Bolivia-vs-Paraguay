# Load necessary library
library(ggplot2)
# Data setup
data <- data.frame(
Country = rep(c("Bolivia", "Paraguay"), each = 7),
Year = rep(c(2023, 2016, 2014, 2012, 2010, 2008, 2006), 2),
Attendance = c(4.2801, 5.4361, 5.1666, 5.3670, 7.7102, 3.7610, 7.7878,
14.4781, 12.7081, 12.0160, 10.5799, 9.8316, 11.1780, 6.8228)
)
# Perform ANOVA
anova_result <- aov(Attendance ~ Country, data = data)
# Perform Tukey's HSD post-hoc test
tukey_result <- TukeyHSD(anova_result)
# Print the Tukey HSD results
print(tukey_result)
# Plot the Tukey HSD results
plot(tukey_result)
# Aligned Bolivia and Paraguay data
bolivia <- c(41.61, 47.52, 42.99, 46.88, 44.53, 54.83, 53.56, 50.99, 47.95)
paraguay <- c(51.67, 49.96, 53.07, 50.39, 58.41, 69.98, 57.97, 57.47, 59.08)
# Perform independent t-test
t_test_result <- t.test(bolivia, paraguay)
# Print the result
print(t_test_result)
