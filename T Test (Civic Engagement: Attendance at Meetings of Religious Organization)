# Remove NA values
data <- na.omit(data)
# Calculate mean and standard error for each country
summary_stats <- data %>%
group_by(Country) %>%
summarise(Mean = mean(Score),
SD = sd(Score),
SE = SD / sqrt(n()),
.groups = 'drop')
# Plotting
ggplot(summary_stats, aes(x = Country, y = Mean, fill = Country)) +
geom_bar(stat = "identity", position = position_dodge(), color = "black", width = 0.5) +
geom_errorbar(aes(ymin = Mean - SE, ymax = Mean + SE), width = 0.2) +
labs(title = "Comparison of Neighborhood Insecurity Between Bolivia and Paraguay",
x = "Country",
y = "Mean Score with Error Bars") +
theme_minimal() +
theme(plot.title = element_text(hjust = 0.5)) +
scale_fill_brewer(palette = "Pastel1") # Aesthetic color scheme
# Data for the ANOVA results
anova_results <- data.frame(
Category = c("Country", "Residuals"),
Df = c(1, 17),
Sum_Sq = c(328.0, 481.5),
Mean_Sq = c(328.0, 28.3),
F_value = c(11.58, NA), # NA for residuals as F value does not apply
Pr_F = c(0.00338, NA) # NA for residuals
)
# Creating the plot
library(ggplot2)
ggplot(anova_results, aes(x = Category, y = F_value, fill = Category)) +
geom_bar(stat = "identity", width = 0.5) +
geom_text(aes(label = sprintf("F = %.2f\np = %.4f", F_value, Pr_F), y = F_value + 1),
position = position_dodge(width = 0.5), vjust = -0.5) +
labs(title = "ANOVA Results: Effect of Country on Perception Scores",
x = "Source of Variation",
y = "F-value") +
theme_minimal() +
theme(plot.title = element_text(hjust = 0.5),
legend.title = element_blank()) +
scale_fill_manual(values = c("Country" = "steelblue", "Residuals" = "grey"))
