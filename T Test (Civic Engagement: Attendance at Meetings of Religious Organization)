Factor = rep(c("Neighborhood Insecurity", "Crime", "Democratic Support", "Coup Justification"), 2),
Score = c(
80, mean(c(75, 70)), mean(c(85, 80, 75)), mean(c(65, 60, 55)), # Bolivia
70, mean(c(65, 60)), mean(c(78, 76, 74)), mean(c(60, 58, 53)) # Paraguay
),
Weight = rep(weights, 2)
)
# Calculate the weighted score for each factor
scores$Weighted_Score <- scores$Score * scores$Weight
# Perform ANOVA
anova_result <- aov(Weighted_Score ~ Country, data = scores)
# Display ANOVA summary
summary(anova_result)
